{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
					"./app",
					"./common/constants",
					"./common/tools",
					"./server"
				],
        "includePattern": ".+\\.js?$",
        "excludePattern": "(node_modules/|docs|__tests__|\\.test\\.js|index\\.js)"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-babel"
    ],
    "babel": {
      "babelrc": true
    },
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "readme": "README.md",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
		"docdash": {
			"static": true,
			"sort": true
		}
}
